{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "EAIMessage": {
      "type": "object",
      "description": "Enterprise Application Integration envelope containing header metadata and business payload",
      "additionalProperties": false,
      "properties": {
        "EAIHeader": {
          "type": "object",
          "description": "Technical and routing metadata required for request processing and tracking",
          "additionalProperties": false,
          "properties": {}
        },
        "Body": {
          "type": "object",
          "description": "Business request payload specific to the service operation",
          "additionalProperties": false,
          "properties": {
            "header": {
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "requesterInformation1": {
                  "type": "string"
                },
                "requesterInformation2": {
                  "type": "string"
                },
                "requesterInformation3": {
                  "type": "string"
                },
                "requesterInformation5": {
                  "type": "string"
                },
                "requesterInformation6": {
                  "type": "string"
                },
                "requesterInformation7": {
                  "type": "string"
                },
                "requesterInformation14": {
                  "type": "string"
                },
                "requesterInformation19": {
                  "type": "string"
                },
                "messageFormat": {
                  "type": "string",
                  "description": "Message payload format (JSON or XML)"
                },
                "channelId": {
                  "type": "string",
                  "description": "Channel from which the request originated"
                },
                "functionId": {
                  "type": "string",
                  "description": "Identifier of the business function being invoked"
                },
                "cif": {
                  "type": "string",
                  "description": "Customer Information File number"
                },
                "language": {
                  "type": "string",
                  "description": "Request language code"
                },
                "eaiReturnCode": {
                  "type": "string"
                },
                "reversalOption": {
                  "type": "string",
                  "description": "Indicates whether the transaction can be reversed"
                },
                "originalMessageId": {
                  "type": "string"
                },
                "frontendReferenceNumber": {
                  "type": "string",
                  "description": "Frontend system reference number"
                },
                "backendReferenceNumber": {
                  "type": "string",
                  "description": "Backend system reference number"
                },
                "timeStamp": {
                  "type": "string"
                },
                "deviceInfo": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "model": {
                      "type": "string"
                    },
                    "city": {
                      "type": "string"
                    },
                    "id": {
                      "type": "string"
                    },
                    "ipAddress": {
                      "type": "string"
                    },
                    "country": {
                      "type": "string"
                    },
                    "osVersion": {
                      "type": "string"
                    }
                  }
                }
              }
            },
            "data": {
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "accountNumber": {
                  "type": "string"
                },
                "startDate": {
                  "type": "string"
                },
                "endDate": {
                  "type": "string"
                },
                "durationInDays": {
                  "type": "string"
                },
                "pageOffset": {
                  "type": "string"
                },
                "pageSize": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
